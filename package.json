{
  "name": "ember-resize-text",
  "version": "1.2.0",
  "description": "ember addon that makes use of ember-text-measurer to efficiently resize the font of a single line of text to make it fit",
  "keywords": [
    "ember-addon",
    "resize",
    "text",
    "font",
    "size"
  ],
  "license": "MIT",
  "author": "Steve Hummingbird",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/st-h/ember-resize-text",
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "dependencies": {
    "coveralls": "^3.0.3",
    "ember-cli-babel": "^7.7.3",
    "ember-cli-htmlbars": "^3.0.1",
    "ember-text-measurer": "^0.5.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.7.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.10.1",
    "ember-cli-code-coverage": "^0.4.2",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-github-pages": "^0.2.0",
    "ember-cli-htmlbars-inline-precompile": "^2.1.0",
    "ember-cli-inject-live-reload": "^2.0.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.3",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator-for-testing": "^1.0.0",
    "ember-qunit": "^4.5.1",
    "ember-resolver": "^5.1.3",
    "ember-source": "~3.10.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "^1.2.1",
    "eslint-plugin-ember": "^6.2.0",
