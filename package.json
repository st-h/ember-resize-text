{
  "name": "ember-resize-text",
  "version": "1.0.4",
  "description": "ember addon that makes use of ember-text-measurer to efficiently resize the font of a single line of text to make it fit within its bounds",
  "keywords": [
    "ember-addon",
    "resize",
    "text",
    "font",
    "size"
  ],
  "license": "MIT",
  "author": "Steve Hummingbird",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/st-h/ember-resize-text",
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember try:each"
  },
  "dependencies": {
    "ember-cli-babel": "^6.11.0",
    "ember-cli-htmlbars": "^2.0.3",
    "ember-text-measurer": "^0.4.0"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "ember-ajax": "^3.0.0",
    "ember-cli": "~2.18.0",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-eslint": "^4.2.3",
    "ember-cli-github-pages": "^0.1.2",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.2.1",
    "ember-cli-release": "^1.0.0-beta.2",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-resolver": "^4.0.0",
    "ember-source": "~2.18.0",
    "loader.js": "^4.2.3"
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
